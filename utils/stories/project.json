{
  "root": "utils/stories",
  "sourceRoot": "utils/stories/src",
  "projectType": "library",
  "targets": {
    "storybook": {
      "configurations": {
        "ci": {
          "quiet": true
        }
      },
      "dependsOn": [
        "vite-plugin-theme:build",
        "vite-plugin-icons:build",
        "elements:build-watch"
      ],
      "executor": "@nx/storybook:storybook",
      "options": {
        "configDir": "utils/stories/all-stories"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "utils/stories/**/*.{ts,mts}",
          "utils/stories/.storybook-shared/*.ts"
        ],
        "quiet": true
      },
      "outputs": [
        "{options.outputFile}"
      ]
    }
  }
}
