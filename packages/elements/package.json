{
  "name": "@ch-ui/elements",
  "version": "0.5.0",
  "description": "A set of (mostly) friendly web components.",
  "bugs": "https://github.com/ch-ui-dev/ch-ui/issues",
  "authors": [
    "Will Shown <w@willshown.com> (https://willshown.com/)"
  ],
  "license": "Ch Public License",
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/elements/elements.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/types/index.d.ts"
    },
    "./loader": {
      "import": "./dist/loader/index.js",
      "require": "./dist/loader/index.cjs",
      "types": "./dist/loader/index.d.ts"
    },
    "./docs": {
      "import": "./dist/docs.json",
      "require": "./dist/docs.json",
      "types": "./dist/docs.d.ts"
    },
    "./ch-default-root.css": {
      "import": "./dist/ch-ui/ch-ui.css",
      "require": "./dist/ch-ui/ch-ui.css"
    }
  },
  "files": [ "dist", "src" ],
  "devDependencies": {
    "@stencil/core": "^4.7.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.12.3"
  }
}
