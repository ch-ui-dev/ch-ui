{
  "name": "@ch-ui/elements",
  "version": "0.5.0",
  "description": "A set of (mostly) friendly web components.",
  "bugs": "https://github.com/ch-ui-dev/ch-ui/issues",
  "authors": [
    "Will Shown <w@willshown.com> (https://willshown.com/)"
  ],
  "license": "Ch Public License",
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/elements/elements.esm.js",
  "exports": {
    ".": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./loader": {
      "import": "./dist/loader/index.js",
      "require": "./dist/loader/index.cjs.js",
      "types": "./dist/loader/index.d.ts"
    },
    "./ch-button": {
      "import": "./dist/components/ch-button.js",
      "types": "./dist/components/ch-button.d.ts"
    },
    "./ch-icon": {
      "import": "./dist/components/ch-icon.js",
      "types": "./dist/components/ch-icon.d.ts"
    },
    "./ch-link": {
      "import": "./dist/components/ch-link.js",
      "types": "./dist/components/ch-link.d.ts"
    },
    "./hydrate": {
      "import": "./dist/hydrate/index.mjs",
      "require": "./dist/hydrate/index.js",
      "types": "./dist/hydrate/index.d.ts"
    },
    "./config": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/types/index.d.ts"
    },
    "./docs": {
      "import": "./dist/docs.json",
      "require": "./dist/docs.json",
      "types": "./dist/docs.d.ts"
    },
    "./ch-default-root.css": {
      "import": "./dist/ch-ui/ch-ui.css",
      "require": "./dist/ch-ui/ch-ui.css"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@stencil/core": "4.19.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.12.3"
  },
  "dependencies": {
    "alea": "^1.0.1"
  },
  "scripts": {
    "stencil:build": "stencil build",
    "stencil:serve": "stencil build --serve --watch --dev",
    "stencil:build-watch": "stencil build --watch"
  }
}
