{
  "name": "docs",
  "root": "apps/docs",
  "sourceRoot": "apps/docs/src",
  "projectType": "application",
  "targets": {
    "build": {
      "command": "cd apps/docs && pnpm run astro:build"
    },
    "serve": {
      "command": "cd apps/docs && pnpm run astro:serve",
      "dependsOn": ["^build", "elements:build-watch"]
    }
  }
}
