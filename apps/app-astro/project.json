{
  "root": "apps/app-astro",
  "sourceRoot": "apps/app-astro/src",
  "projectType": "application",
  "targets": {
    "build": {
      "command": "cd apps/app-astro && ./node_modules/.bin/astro build"
    },
    "serve": {
      "command": "cd apps/app-astro && ./node_modules/.bin/astro dev"
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "format": "unix",
        "lintFilePatterns": [
          "apps/app-astro/**/*.{ts,mts}",
          "apps/app-astro/.storybook-shared/*.ts"
        ],
        "quiet": true
      },
      "outputs": [
        "{options.outputFile}"
      ]
    }
  }
}
